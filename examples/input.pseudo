output "Input a greeting."
input GREETING
output GREETING, "to you too!"

output "Input an integer."
input COUNT as int

if COUNT mod 2 = 0 then
    output COUNT, "is even..."
else
    output COUNT, "is odd..."
end if

output "Input a float."
input NUMBER as float

loop I from 0 to 10
    output I * NUMBER / 10
end loop
